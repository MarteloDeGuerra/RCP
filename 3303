#include <stdio.h>
#include <string.h>

int main() {
    char palavra[21];
    while (scanf("%s", palavra) != EOF) {

        int tamanhoPalavra = strlen(palavra);

        if (tamanhoPalavra >= 10) {
            printf("palavrao\n");
        } else {
            printf("palavrinha\n");
        }
    }
    return 0;
}
